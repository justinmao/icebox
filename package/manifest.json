{
  "manifest_version": 2,

  "name": "Icebox - Cold Storage for Browsing Sessions",
  "short_name": "Icebox",
  "version": "1.0",

  "icons": { "16": "assets/icon16.png",
           "48": "assets/icon48.png",
          "128": "assets/icon128.png" },

  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs",
    "storage",
    "http://*/",
    "https://*/"
  ]
}
